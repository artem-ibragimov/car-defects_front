{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "checkJs": true,
    "allowJs": true,
    "baseUrl": "./",
    "lib": [
      "es2019",
      "dom",
      "dom.iterable"
    ],
    "types": [
      "svelte",
      "node"
    ]
  },
  "include": [
    "src"
, "global.d.ts"  ],
  "exclude": [
    "node_modules",
    "__sapper__/*",
    "public/*"
  ]
}