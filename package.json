		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"axios": "^1.4.0",
		"chart.js": "^4.3.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"flat": "^5.0.2",
		"imagemin-keep-folder": "^5.3.2",
